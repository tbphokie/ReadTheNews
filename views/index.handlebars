<div class="container">
    <div class="row">
        <div class="col-sm-6">
            <div class="table table-responsive">
                <tbody id="articles">
                    {{#each article}}
                        <tr>
                            <td>
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h3 class="panel-title">{{this.title}}</h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="container">
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    {{this.link}}
                                                </div>
                                                <div class="col-sm-6">
                                                    {{this.summary}}<br>
                                                    {{this.author}}<br>
                                                    {{this.dateline}}<br>
                                                    {{this.link}}<br>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </td>
                        </tr>
                </tbody>
            </div>
        </div>
    </div>
</div>